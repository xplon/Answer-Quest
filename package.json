{
  "name": "universal-quiz-system",
  "version": "1.0.0",
  "description": "一个通用的web答题系统，支持单选、多选和判断题",
  "main": "app.js",
  "scripts": {
    "start": "python -m http.server 8080",
    "dev": "python -m http.server 8080",
    "convert": "node converter.js",
    "test": "node -e \"const { runAllTests } = require('./test.js'); runAllTests();\"",
    "subject:list": "node subject-manager.js list",
    "subject:create": "node subject-manager.js create",
    "subject:switch": "node subject-manager.js switch",
    "subject:delete": "node subject-manager.js delete"
  },
  "keywords": [
    "quiz",
